<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="tptAppKodutoo.Views.koolist"
             xmlns:views="clr-namespace:tptAppKodutoo.Views"
             xmlns:viewModels="clr-namespace:tptAppKodutoo.ViewModels;assembly=tptAppKodutoo"
             Title="{Binding Title}">
    <ContentPage.BindingContext>
        <viewModels:KoolistViewModel/>
    </ContentPage.BindingContext>
    
    <ContentPage.Content>
        <WebView Source="https://www.tptlive.ee/kool/pohivaartused/" WidthRequest="800" HeightRequest="300" ></WebView>
    </ContentPage.Content>
</ContentPage>
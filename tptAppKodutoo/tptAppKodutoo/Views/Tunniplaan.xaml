<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="tptAppKodutoo.Views.Tunniplaan"
             xmlns:views="clr-namespace:tptAppKodutoo.Views"
             xmlns:viewModels="clr-namespace:tptAppKodutoo.ViewModels;assembly=tptAppKodutoo"
             Title="{Binding Title}">
    <ContentPage.BindingContext>
        <viewModels:TunniplaanViewModel/>
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
            
            <WebView Source="https://tpt.siseveeb.ee/veebivormid/tunniplaan"
                     HeightRequest="800"
                     WidthRequest="300" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>